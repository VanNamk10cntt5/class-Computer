class Computer:
    def __init__(self, brand, price):
        self.brand = brand
        self.price = price

    def display_info(self):
        print(f"Brand: {self.brand}")
        print(f"Price: {self.price}")

    def turn_on(self):
        print("Computer is turning on.")

    def turn_off(self):
        print("Computer is turning off.")
